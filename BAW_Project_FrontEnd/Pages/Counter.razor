    private async void IncrementCount()
    {
        HttpClient httpClient = new HttpClient();
        BAW_Project_API bAW_Project_API = new BAW_Project_API("https://localhost:7131", httpClient);
        var a = await bAW_Project_API.BookGETAsync(1);
    }
}
